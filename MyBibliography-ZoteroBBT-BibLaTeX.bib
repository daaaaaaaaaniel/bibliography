@article{zook.etal2018_NewS,
  title = {New Spaces of Disruption? {{The}} Failures of {{Bitcoin}} and the Rhetorical Power of Algorithmic Governance},
  shorttitle = {New Spaces of Disruption?},
  author = {Zook, Matthew A. and Blankenship, Joe},
  date = {2018-11-01},
  journaltitle = {Geoforum},
  shortjournal = {Geoforum},
  volume = {96},
  pages = {248--255},
  issn = {0016-7185},
  doi = {10.1016/j.geoforum.2018.08.023},
  url = {https://www.sciencedirect.com/science/article/pii/S0016718518302574},
  urldate = {2022-03-14},
  abstract = {In less than a decade Bitcoin and the technology of blockchain \textendash{} a cryptographically-secured, algorithmically-regulated, distributed-ledger \textendash{} emerged as the enfant terrible of the global economy. Ironically, as cryptocurrencies reached collective valuations of hundreds of billions of dollars the Bitcoin project failed in its original purpose as an alternative currency governed by code rather than trust. Not only has Bitcoin not become a popular means of global peer-to-peer transactions but the much vaulted purity of algorithmic governance is heavily entangled in social relations. This article reviews blockchain's computer architectures, its connections to materiality and space and the complexity of its established practices. This analysis shows that rather than occupying an algorithmic place apart, blockchain contains multiple and conflicting agencies and is messily embedded in the code/space of materiality. Nevertheless the faith in the superiority of algorithmic governance has injected a powerful discourse in economies that has proven more important and disruptive than the actual practices of Bitcoin or blockchain.},
  langid = {english},
  keywords = {Bitcoin,Blockchain,Code/space,Disruption,Financial technologies,Fintech,Innovation}
}

