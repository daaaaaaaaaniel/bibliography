@book{taylor2018_Abid,
  title = {Abiding {{Grace}}: {{Time}}, {{Modernity}}, {{Death}}},
  shorttitle = {Abiding {{Grace}}},
  author = {Taylor, Mark C.},
  date = {2018-10},
  series = {Religion and {{Postmodernism}}},
  publisher = {{University of Chicago Press}},
  location = {{Chicago, IL}},
  url = {https://press.uchicago.edu/ucp/books/book/chicago/A/bo28482108.html},
  urldate = {2021-12-21},
  abstract = {Post-war, post-industrialism, post-religion, post-truth, post-biological, post-human, post-modern. What succeeds the post- age? Mark C. Taylor returns here to some of his central philosophical preoccupations and asks: What comes after the end?~Abiding Grace navigates the competing Hegelian and Kierkegaardian trajectories born out of the Reformation and finds Taylor arguing from spaces in between, showing how both narratives have shaped recent philosophy and culture. For Hegel, Luther's internalization of faith anticipated the modern principle of autonomy, which reached its fullest expression in speculative philosophy.~ The closure of the Hegelian system still endures in the twenty-first century in consumer society, financial capitalism, and virtual culture. For Kierkegaard, by contrast, Luther's God remains radically transcendent, while finite human beings and their world remain fully dependent. From this insight, Heidegger and Derrida developed an alternative view of time in which a radically open future breaks into the present to transform the past, demonstrating that, far from autonomous, life is a gift from an Other that can never be known. Offering an alternative genealogy of deconstruction that traces its pedigree back to readings of Paul by way of Luther, Abiding Grace presents a thoroughgoing critique of modernity and postmodernity's will to power and mastery. In this new philosophical and theological vision, history is not over and the future remains endlessly open.},
  isbn = {978-0-226-56908-6},
  langid = {english},
  pagetotal = {304},
  keywords = {autonomy,consumer society,cultural studies,culture,death,faith,father son holy spirit,financial capitalism,georg wilhelm friedrich hegel,gift,hegelian,history,jacques derrida,kierkegaardian,luther,martin heidegger,mastery,mirror stage,modernism,modernity,other,philosophical,philosophy,post,postmodern,postmodernism,postmodernity,power,reformation,religion,religious,soren kierkegaard,speculation,speculative,structure,theological,theology,time,trinity},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Taylor 2018 - Abiding Grace.pdf}
}

