@article{vidan.etal2019_Mine,
  title = {Mine the Gap: {{Bitcoin}} and the Maintenance of Trustlessness},
  shorttitle = {Mine the Gap},
  author = {Vidan, Gili and Lehdonvirta, Vili},
  date = {2019-01-01},
  journaltitle = {New Media \& Society},
  shortjournal = {New Media \& Society},
  volume = {21},
