@article{baldwin2018_Digi,
  title = {In Digital We Trust: {{Bitcoin}} Discourse, Digital Currencies, and Decentralized Network Fetishism},
  shorttitle = {In Digital We Trust},
  author = {Baldwin, Jon},
  date = {2018-02-13},
  journaltitle = {Palgrave Communications},
  shortjournal = {Palgrave Commun},
  volume = {4},
  number = {1},
  pages = {1--10},
  publisher = {{Palgrave}},
  issn = {2055-1045},
  doi = {10.1057/s41599-018-0065-0},
  url = {https://www.nature.com/articles/s41599-018-0065-0},
  urldate = {2021-12-21},
  abstract = {This paper outlines how the digital currency and network technology of bitcoin functions and explores the context from which it emerged. Bitcoin was conceived in 2008 as an attempt to alleviate trust in government and banks which was at a low during this period of financial crisis. However, with bitcoin trust does not dissipate, rather it shifts. Trust moves from trust in banks or states to trust in algorithms and encryption software. There is a move from conventional trust in the gold standard\textemdash ``In Gold We Trust''\textemdash to the trust announced on U.S. currency\textemdash ``In God We Trust''\textemdash to trust in software and networks\textemdash ``In Digital We Trust''. The hyperbole of bitcoin discourse is deemed to be an expression of the Californian Ideology, which itself often conceals a right-wing agenda. The paper analyses the hype behind the celebration of decentralised digital networks. It proposes that a form of network fetishism operates here. The failure of bitcoin as a currency (rather than as a hoarded commodity in an emergent bubble) and as an idea might be attributed to the failure to see how ultra-modern digital networks conceal very traditional consolidation of power and capital. The rise and fall of bitcoin, in terms of its original ambition, serves as a cautionary tale in the digital age\textemdash it reveals how ingenious innovations that might challenge power and the consolidation of capital become co-opted and colonised by capital. Finally, the paper offers a discussion of the possible progressive uses of the digital technology bitcoin has facilitated.},
  issue = {1},
  langid = {english},
  keywords = {Cultural and media studies,Economics,Science,technology and society},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Cultural and media studies;Economics;Science, technology and society Subject\_term\_id: cultural-and-media-studies;economics;science-technology-and-society},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Baldwin 2018 - In digital we trust.pdf}
}

@book{balestrini.etal2021_Gold,
  title = {The {{Golden Horde}}: {{Revolutionary Italy}}, 1960-1977},
  shorttitle = {The {{Golden Horde}}},
  editor = {Balestrini, Nanni and Moroni, Primo},
  translator = {Braude, {and} with an Introduction by Richard},
  date = {2021-05},
