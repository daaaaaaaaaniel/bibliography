@article{whitham2018_Thin,
  title = {Thinking the '{{Culture Wars}}' and the {{Present Political Crisis}} with the {{Young Marx}} (and {{Friends}})},
  author = {Whitham, Ben},
  date = {2018-05-04},
  journaltitle = {tripleC: Communication, Capitalism \& Critique},
  volume = {16},
  number = {2},
  pages = {707--716},
  issn = {1726-670X},
  doi = {10.31269/triplec.v16i2.981},
  url = {https://www.triple-c.at/index.php/tripleC/article/view/981},
  urldate = {2022-06-29},
  abstract = {We stand at a key juncture: a Western political crisis arose in 2016-17 to match the deep economic crisis of the preceding decade. Events and new social movements of recent years seem to hail the collapse of the project of liberal democracy, though it is hard to see what will replace it. Among the conceptual and analytic tools bequeathed by Marx are those necessary to better understand and anticipate the direction of this key historical moment \textendash{} from Donald Trump, Brexit and the so-called `culture wars' to the horizon of liberal democracy itself. In this reflection, I suggest some ways in which Marx's early thoughts on the liberal state and civil society can and should help us to better understand and explain our present predicament. To say that the Young Marx can help us today with what he called `the ruthless critique of everything existing' is not to say that he can do so alone. It is precisely the issues overlooked or `fudged' by Marx and Marxism \textendash{} gender, sexuality, and race/racism for example \textendash{} that now sit at the centre of our `culture wars', alongside but never reducible to the contradictions and crises of capitalism. I conclude that it is only with the help of other writers of the 20th and 21st centuries, from Antonio Gramsci to Frantz Fanon and bell hooks, that we can usefully mobilise the Young Marx today, to critique the world as we find it and especially \textendash{} the very `point' of theory according to Marx \textendash{} to change it.},
  langid = {english},
  keywords = {bell hooks,culture wars,intersectionality,Young Marx},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Journal Article/tripleC- Communication, Capitalism & Critique/vol. 16/no. 2/Whitham 2018 - Thinking the 'Culture Wars' and the Present Political Crisis with the Young Marx (and Friends) [whitham2018_Thin].pdf}
}

