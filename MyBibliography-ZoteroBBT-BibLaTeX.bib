@article{mylonas2012_Medi,
  title = {Media and the {{Economic Crisis}} of the {{EU}}: {{The}} `{{Culturalization}}' of a {{Systemic Crisis}} and {{Bild-Zeitung}}'s {{Framing}} of {{Greece}}},
  shorttitle = {Media and the {{Economic Crisis}} of the {{EU}}},
  author = {Mylonas, Yiannis},
  date = {2012-07-15},
  journaltitle = {tripleC: Communication, Capitalism \& Critique},
  volume = {10},
