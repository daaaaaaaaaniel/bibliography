@article{haiven2021_Feti,
  title = {The {{Fetish Revisited}}: {{Marx}}, {{Freud}} and the {{Gods Black People Make}} by {{J}}. {{Lorand Matory}}},
  shorttitle = {The {{Fetish Revisited}}},
  author = {Haiven, Max},
  date = {2021-11-02},
  journaltitle = {Journal of Cultural Economy},
  volume = {14},
