@article{wilkins2021_Gent,
  title = {Gentrification by Genre? {{The Berlin}} Rom-Com},
  shorttitle = {Gentrification by Genre?},
  author = {Wilkins, Kim},
  date = {2021-05-24},
  journaltitle = {NECSUS\_European Journal of Media Studies},
  volume = {10},
  number = {1},
  pages = {27--54},
  publisher = {{Amsterdam University Press}},
  doi = {10.25969/mediarep/16264},
  url = {https://mediarep.org/handle/doc/17117},
  urldate = {2022-02-20},
  abstract = {Over the past few decades, there has been a significant uptick in the number of people relocating to Berlin. This influx is most of-ten viewed as a response to rebranding the reunified German capi-tal as a creative city \textendash{} a tactic that foregrounded Berlin's longstand-ing reputation for cheap rent, liberal attitudes, artistic culture, and vibrant nightlife. The housing market responded as vacancies plummeted while rent prices skyrocketed. Alongside the widely lamented changing face of the reunified capital, the spike in rent prices is one tangible outcome of Berlin's rapid gentrification. This essay examines the aesthetics of gentrifying Berlin through an ex-amination of a genre commonly associated with the imperatives of gentrification: the romantic comedy. Unlike other cinema tradi-tions associated with urban space, the romcom is commonly un-derstood as a genre that frames the city as a site of aspirational af-fluence and consumerism. This framing has, to date, overwhelm-ingly referred to romcoms produced in the American context. Through analyses of three romcoms set in Berlin \textendash{} Germany's highest grossing romcom to date KEINOHRHASEN (`Rabbit With-out Ears', Schweiger, 2007); the 2019 installment in Emmanuel Benbihy's `City of Love' anthology film series, BERLIN, I LOVE YOU and Doris D\"orrie's GL\"UCK (`Bliss', 2012) this essay interro-gates whether romcoms set in Berlin can be, as has been claimed of their US counterparts, understood as a genre of gentrification.},
  langid = {english},
  annotation = {Accepted: 2021-07-16T08:59:41Z},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Wilkins 2021 - Gentrification by genre.pdf}
}

