  keywords = {Critical Theory,Digital Ethnography,Flickr,Immaterial Labour,Internet,Produsage,Research Methods,Social Media,Web 2.0},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Journal Article/tripleC- Communication, Capitalism & Critique/vol. 10/no. 2/Brown & Quan-Haase 2012 - 'A Workers’ Inquiry 2.0' [brown.etal2012_AWor].pdf}
