  keywords = {alternative media,critical Internet studies,fetishism,ideology,Marx,privacy,private property,Social networking sites,surveillance},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Journal Article/tripleC- Communication, Capitalism & Critique/vol. 10/no. 2/Sevignani 2012 - The Problem of Privacy in Capitalism and the Alternative Social Networking Site Diaspora [sevignani2012_Prob].pdf}
