@book{gray.etal2019_Ghos,
  title = {Ghost {{Work}} : {{How}} to {{Stop Silicon Valley From Building}} a {{New Global Underclass}}},
  shorttitle = {Ghost {{Work}}},
  author = {Gray, Mary L. and Suri, Siddharth},
  date = {2019},
  publisher = {{Mariner Books}},
