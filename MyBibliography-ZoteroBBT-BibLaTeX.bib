@book{kordela2017_Epis,
  title = {Epistemontology in {{Spinoza-Marx-Freud-Lacan}}: {{The}} ({{Bio}}){{Power}} of {{Structure}}},
  shorttitle = {Epistemontology in {{Spinoza-Marx-Freud-Lacan}}},
