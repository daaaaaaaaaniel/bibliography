@article{grossman2019_Pass,
  title = {Passing Cash from Bank Notes to Bitcoin: Standardizing Money},
  shorttitle = {Passing Cash from Bank Notes to Bitcoin},
  author = {Grossman, Jonathan H.},
  date = {2019-07-04},
  journaltitle = {Journal of Cultural Economy},
  volume = {12},
