@article{manzerolle.etal2012_Comm,
  title = {The {{Communication}} of {{Capital}}: {{Digital Media}} and the {{Logic}} of {{Acceleration}}},
  shorttitle = {The {{Communication}} of {{Capital}}},
  author = {Manzerolle, Vincent R. and Kj\o sen, Atle Mikkola},
  date = {2012-05-25},
  journaltitle = {tripleC: Communication, Capitalism \& Critique},
  volume = {10},
  number = {2},
  pages = {214--229},
  issn = {1726-670X},
  doi = {10.31269/triplec.v10i2.412},
  url = {https://www.triple-c.at/index.php/tripleC/article/view/412},
  urldate = {2022-06-29},
  abstract = {This paper argues that questions concerning the circulation of capital are central to the study of contemporary and future media under capitalism. Moreover, it argues that such questions have been central to Marx's analysis of the reproduction of capital vis-\`a-vis the realization of value and the reduction of circulation time. Marx's concepts of both the circuit and circulation of capital implies a theory of communication. Thus the purpose of our paper is to outline the logistical mechanisms that underlie a Marxist theory of media and communication and thereby foregrounding the role new media plays in reducing circulation time. We argue that the necessity of theorizing communication from a circuit and circulation-centric point of view stems from the emergence of a number of new technological phenomena that intensify, but sometimes undermine, the capitalist logic of acceleration. For the purposes of understanding the evolution of digital technologies, ostensibly employed to accelerate the circulation of capital\textemdash or put differently, to reduce circulation time\textemdash we need to pay attention to volume 2 of Capital, and key sections in the Grundrisse.},
  langid = {english},
  keywords = {acceleration,Capital Volume 2,convergence,cybernetics and dromoeconomics.,diagrammatics,digital media,Grundrisse,marxism,media theory,mobile payment systems},
  file = {/Users/daniel/Documents/Text Repository/ZotFile/Journal Article/tripleC- Communication, Capitalism & Critique/vol. 10/no. 2/Manzerolle & Kjøsen 2012 - The Communication of Capital [manzerolle.etal2012_Comm].pdf}
}

