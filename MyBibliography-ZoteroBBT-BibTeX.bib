@book{Kafka2012_DemonWriting,
	address = {Brooklyn},
	series = {Zone {Books}},
	title = {The {Demon} of {Writing} : {Powers} and {Failures} of {Paperwork}},
	isbn = {978-1-935408-26-0},
	url = {https://login.ezproxy.pratt.edu/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=edsebk&AN=2431029&site=eds-live&scope=site},
	abstract = {Since the middle of the eighteenth century, political thinkers of all kinds — radical and reactionary, professional and amateur — have been complaining about “bureaucracy.” But what, exactly, is all this complaining about?The Demon of Writing is a critical history and theory of one of the most ubiquitous, least understood forms of media: paperwork. States rely on records to tax and spend, protect and serve, discipline and punish. But time and again this paperwork proves to be unreliable. Examining episodes from the story of a clerk who lost his job and then his mind in the French Revolution to Roland Barthes's brief stint as a university administrator, the book reveals the powers, failures, and even pleasures of paperwork. Many of its complexities, the book argues, have been obscured by the comic-paranoid style that characterizes so many of our criticisms of bureaucracy. At the same time, the book outlines a new theory of what Marx called the “bureaucratic medium.” Returning first to Marx, then to Freud, The Demon of Writing argues that this theory of paperwork must be attentive to both praxis and parapraxis.},
	language = {English},
	publisher = {Zone Books},
	author = {Kafka, Ben},
	year = {2012},
	keywords = {HISTORY / Social History, SOCIAL SCIENCE / Media Studies},
	file = {Kafka 2012 - The Demon of Writing Powers and Failures of Paperwork.pdf:/Users/daniel/Documents/Text Repository/ZotFile/Kafka 2012 - The Demon of Writing Powers and Failures of Paperwork.pdf:application/pdf},
