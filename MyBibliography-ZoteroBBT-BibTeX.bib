	keywords = {capital, labor, commons, value, bienes comunes, communaux (biens communs), déchets, desperdicio, dialectic, dialéctica, dialectique, labeur, trabajo, valeur, valor, waste},
	pages = {773--783},
	file = {Gidwani 2013 - Six theses on waste, value, and commons.pdf:/Users/daniel/Documents/Text Repository/ZotFile/Gidwani 2013 - Six theses on waste, value, and commons.pdf:application/pdf},
