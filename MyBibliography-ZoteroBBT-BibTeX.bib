@book{Henderson2013_ValueMarx,
	title = {Value in {Marx}: {The} {Persistence} of {Value} in a {More}-{Than}-{Capitalist} {World}},
	isbn = {978-0-8166-8095-5},
	shorttitle = {Value in {Marx}},
	url = {https://minnesota.universitypressscholarship.com/10.5749/minnesota/9780816680955.001.0001/upso-9780816680955},
	abstract = {Value in Marx shows on the one hand how Marx draws out the relevance of value not just to capitalism but any mode of production, and on the other hand identifies the problematic nature of Marx’s investments in the value concept and the political imaginaries that might be able to confront them. Drawing upon Capital and the Grundrisse, Henderson shows how these texts do not give rise to a single theory of value–the traditional reading which renders value and capital as virtually synonymous and which reduces value theory to a mere economic theory–but rather are rich in multiple value-theoretic explorations that extend beyond capitalism. While these explorations address in various ways the concatenation of people and things that produce different forms of social becoming, they also impose the necessity that we recognize the limits of becoming that would be produced by all modes of production (not just capitalism). At stake then is how these limits might be confronted in a creative, even joyful, way. Can they, in other words, become part and parcel of what we desire, rather than being ignored and tucked away in the realm of denial and of that which cannot be spoken? In a final interpretive account of the plurality of Marx’s value-theoretic meditations, Henderson argues that such a reading, in which Marx produces an affective relationship to the idea of social limits, is possible.},
	language = {eng},
	urldate = {2022-02-28},
	publisher = {University of Minnesota Press},
	author = {Henderson, George},
	year = {2013},
	doi = {10.5749/minnesota/9780816680955.001.0001},
	keywords = {Capital, Marx, value, Grundrisse, social limits},
	file = {Henderson 2013 - Value in Marx.pdf:/Users/daniel/Documents/Text Repository/ZotFile/Henderson 2013 - Value in Marx.pdf:application/pdf},
}

