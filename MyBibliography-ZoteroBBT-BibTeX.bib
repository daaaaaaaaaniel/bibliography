@article{Laurie&Clayton2004_ProofofWorkProves,
	title = {"{Proof}-of-{Work}" {Proves} {Not} to {Work}},
	url = {https://www.cl.cam.ac.uk/~rnc1/proofwork.pdf},
	abstract = {A frequently proposed method of reducing unsolicited bulk email (“spam”) is for senders to pay for each email they send. Proof-ofwork schemes avoid charging real money by requiring senders to demonstrate that they have expended processing time in solving a cryptographic puzzle. We attempt to determine how diﬃcult that puzzle should be so as to be eﬀective in preventing spam. We analyse this both from an economic perspective, “how can we stop it being cost-eﬀective to send spam”, and from a security perspective, “spammers can access insecure end-user machines and will steal processing cycles to solve puzzles”. Both analyses lead to similar values of puzzle diﬃculty. Unfortunately, realworld data from a large ISP shows that these diﬃculty levels would mean that signiﬁcant numbers of senders of legitimate email would be unable to continue their current levels of activity. We conclude that proof-of-work will not be a solution to the problem of spam.},
	language = {en},
	number = {Third Annual Workshop on Economics and Information Security (WEIS04)},
	author = {Laurie, Ben and Clayton, Richard},
	month = may,
	year = {2004},
	pages = {9},
