	keywords = {LITERARY CRITICISM / Comparative Literature, Comic, The, in literature, Laughter in literature, Laughter--Psychological aspects, LITERARY CRITICISM / Humor, PHILOSOPHY / History \& Surveys / Modern, PSYCHOLOGY / Movements / Psychoanalysis},
}

