	file = {Fanti et al. 2018 - Compounding of Wealth in Proof-of-Stake Cryptocurrencies.pdf:/Users/daniel/Zotero/storage/GJK2A43Z/Fanti et al. 2018 - Compounding of Wealth in Proof-of-Stake Cryptocurrencies.pdf:application/pdf},
